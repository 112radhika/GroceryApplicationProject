<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"> 
<suite name="ParellelTesting" parallel="methods">
  <parameter name="browsers" value="Chrome"></parameter>
  <test thread-count="5" name="Test1">
    <classes> 
      <class name="testscripts.LoginTest">
       <methods><include name="verifyUserLoginWithValidCredentials"></include></methods>
      </class>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Test2">
    <classes> 
      <class name="testscripts.LoginTest">
       <methods><include name="verifyUserLoginWithValidUsernameAndInvalidPassword"></include></methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
 