<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"> 
<!--initially hover on https tag and click on force download to remove errors in the file -->
<suite name="Suite">
<!-- add the listeners tag and then add the details in <listener> tag-->
  <listeners> 
    <listener class-name="reportGeneration.Listeners"></listener>
  </listeners>
  <parameter name="browsers" value="Firefox"></parameter>
  <test thread-count="5" name="Test">
    <classes> <!-- mention the name of the testcases to be executed => packagename.testclassname -->
      <!--<class name="testscripts.HomeTest">
      	<methods> use include tag to selectively execute testcase
      		<include name="verifyUserIsAbleToLogout"></include>
      	</methods>
      </class>-->
      <class name="testscripts.LoginTest">
        <methods>
          <!--use exclude tag to selectively not execute testcase-->
          <exclude name="verifyUserLoginWithValidUsernameAndInvalidPassword"></exclude>
        </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
 